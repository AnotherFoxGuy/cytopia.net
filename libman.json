{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/scripts/jquery",
      "files": [
        "dist/jquery.min.js"
      ]
    },
    {
      "library": "foundation-sites@6.7.5",
      "destination": "input/stylesheets/foundation",
      "files": [
        "scss/**/*.scss",
        "_vendor/**/*"
      ]
    },
    {
      "library": "motion-ui@2.0.4",
      "destination": "input/stylesheets/motion-ui",
      "files": [
        "src/**/*.scss"
      ]
    }
  ]
}